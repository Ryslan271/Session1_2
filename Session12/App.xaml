<Application x:Class="Session12.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Session12"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="StyleGrid">
            <Setter Property="TextBlock.Foreground" Value="#FF2B2B2B"/>
            <Setter Property="TextBlock.FontFamily" Value="Bahnschrift SemiBold"/>
        </Style>
    </Application.Resources>
</Application>
